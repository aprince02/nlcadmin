<div class="form-group row">
    <label class="col-form-label col-sm-2" for="first_name">First Name</label>
    <div class="col-sm-8">
      <input required type="text" autofocus class="form-control" name="first_name" value="<%= member.first_name %>" />
    </div>
  </div>
  
  <div class="form-group row">
    <label class="col-form-label col-sm-2" for="surname">Surname</label>
    <div class="col-sm-7">
      <input required type="text" class="form-control" name="surname" value="<%= member.surname %>" />
    </div>
  </div>
  
  <div class="form-group row">
    <label class="col-form-label col-sm-2" for="sex">SEX</label>
    <div class="col-sm-10">
      <select required class="form-control" name="sex" id="sex">
        <option selected value="<%= member.sex %>"><%= member.sex %></option>
        <option value="MALE">MALE</option>
        <option value="FEMALE">FEMALE</option>
      </select>
    </div>
  </div>

  <div class="form-group row">
    <label class="col-form-label col-sm-2" for="email">Email</label>
    <div class="col-sm-7">
      <input required type="email" class="form-control" name="email" value="<%= member.email %>" />
    </div>
  </div>

  <div class="form-group row">
    <label class="col-form-label col-sm-2" for="phone_number">Phone Number</label>
    <div class="col-sm-7">
      <input required type="text" class="form-control" name="phone_number" value="<%= member.phone_number %>" />
    </div>
  </div>

  <div class="form-group row">
    <label class="col-form-label col-sm-2" for="address_line_1">Address Line 1</label>
    <div class="col-sm-7">
      <input type="text" class="form-control" name="address_line_1" value="<%= member.address_line_1 %>" />
    </div>
  </div>

  <div class="form-group row">
    <label class="col-form-label col-sm-2" for="address_line_2">Address Line 2</label>
    <div class="col-sm-7">
      <input type="text" class="form-control" name="address_line_2" value="<%= member.address_line_2 %>" />
    </div>
  </div>

  <div class="form-group row">
    <label class="col-form-label col-sm-2" for="city">City</label>
    <div class="col-sm-7">
      <input type="text" class="form-control" name="city" value="<%= member.city %>" />
    </div>
  </div>

  <div class="form-group row">
    <label class="col-form-label col-sm-2" for="postcode">Postcode</label>
    <div class="col-sm-7">
      <input type="text" class="form-control" name="postcode" value="<%= member.postcode %>" />
    </div>
  </div>

  <div class="form-group row">
    <label class="col-form-label col-sm-2" for="date_of_birth">Date of Birth</label>
    <div class="col-sm-10">
      <input required type="date" class="form-control" cols="20" name="date_of_birth" maxlength="32000" value="<%= member.date_of_birth %>" />
    </div>
  </div>

  <div class="form-group row">
    <label class="col-form-label col-sm-2" for="baptised">Baptised</label>
    <div class="col-sm-10">
      <select required class="form-control" name="baptised" id="baptised">
        <option selected value="<%= member.baptised %>"><%= member.baptised %></option>
        <option value="YES">YES</option>
        <option value="NO">NO</option>
      </select>
    </div>
  </div>

  <div class="form-group row">
    <label class="col-form-label col-sm-2" for="baptised_date">Date of Baptism</label>
    <div class="col-sm-10">
      <input required type="date" class="form-control" cols="20" name="baptised_date" maxlength="32000" value="<%= member.baptised_date %>" />
    </div>
  </div>

  <div class="form-group row">
    <label class="col-form-label col-sm-2" for="holy_spirit">Annointed with Holy Spirit</label>
    <div class="col-sm-10">
      <select required class="form-control" name="holy_spirit" id="holy_spirit">
        <option selected value="<%= member.holy_spirit %>"><%= member.holy_spirit %></option>
        <option value="YES">YES</option>
        <option value="NO">NO</option>
      </select>
    </div>
  </div>

  <div class="form-group row">
    <label class="col-form-label col-sm-2" for="native_church">Native Church</label>
    <div class="col-sm-7">
      <input type="text" class="form-control" name="native_church" value="<%= member.native_church %>" />
    </div>
  </div>

  <div class="form-group row">
    <label class="col-form-label col-sm-2" for="children_details">Children Details</label>
    <div class="col-sm-7">
      <input type="text" class="form-control" name="children_details" value="<%= member.children_details %>" />
    </div>
  </div>

  <div class="form-group row">
    <label class="col-form-label col-sm-2" for="emergency_contact_1">Emergency Contact Number 1</label>
    <div class="col-sm-7">
      <input required type="text" class="form-control" name="emergency_contact_1" value="<%= member.emergency_contact_1 %>" />
    </div>
  </div>

  <div class="form-group row">
    <label class="col-form-label col-sm-2" for="emergency_contact_1_name">Emergency Contact Number 1 Name</label>
    <div class="col-sm-7">
      <input required type="text" class="form-control" name="emergency_contact_1_name" value="<%= member.emergency_contact_1_name %>" />
    </div>
  </div>

  <div class="form-group row">
    <label class="col-form-label col-sm-2" for="emergency_contact_2">Emergency Contact Number 2</label>
    <div class="col-sm-5">
      <input required type="text" style="max-width: fit-content;" class="form-control" name="emergency_contact_2" value="<%= member.emergency_contact_2 %>" />
    <label class="col-form-label col-sm-2" for="emergency_contact_2_name">Emergency Contact Number 2 Name</label>
      <input required type="text"  style="max-width: fit-content;"class="form-control" name="emergency_contact_2_name" value="<%= member.emergency_contact_2_name %>" />
    </div>
  </div>

  <div class="form-group row">
    <label class="col-form-label col-sm-2" for="emergency_contact_2_name">Emergency Contact Number 2 Name</label>
    <div class="col-sm-7">
      <input required type="text" class="form-control" name="emergency_contact_2_name" value="<%= member.emergency_contact_2_name %>" />
    </div>
  </div>